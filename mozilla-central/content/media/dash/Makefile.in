DEPTH      := @DEPTH@
topsrcdir  := @top_srcdir@
srcdir     := @srcdir@
VPATH      := @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE         := content
LIBRARY_NAME   := gkcondash_s
LIBXUL_LIBRARY := 1

EXPORTS := \
  DASHDecoder.h \
  DASHRepDecoder.h \
  DASHReader.h \
  DASHRepReader.h \
  $(NULL)

CPPSRCS := \
  DASHDecoder.cpp \
  DASHRepDecoder.cpp \
  DASHReader.cpp \
  $(NULL)

FORCE_STATIC_LIB := 1

include $(topsrcdir)/config/rules.mk

LOCAL_INCLUDES := \
  -I$(topsrcdir)/netwerk/dash/mpd \
  -I$(srcdir)/../webm \
  -I$(srcdir)/../../base/src \
  -I$(srcdir)/../../html/content/src \
  $(MOZ_LIBVPX_INCLUDES) \
  $(NULL)
