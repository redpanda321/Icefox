relativesrcdir  = @relativesrcdir@

include $(DEPTH)/config/autoconf.mk

MOCHITEST_FILES = 	test_bug423375.html \
                test_bug246699.html \
                test_bug292789.html \
                test_bug470804.html \
                test_disallowInheritPrincipal.html \
