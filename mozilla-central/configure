        echo "configure: warning: Removing gnomevfs from MOZ_EXTENSIONS due to no --enable-gnomevfs." 1>&2
    fi
    MOZ_EXTENSIONS=`echo $MOZ_EXTENSIONS | sed -e 's|gnomevfs||'`
fi

