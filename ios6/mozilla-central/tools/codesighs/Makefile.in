DEPTH		= @DEPTH@
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk


CSRCS += \
		codesighs.c \
		maptsvdifftool.c \
		$(NULL)

ifeq ($(OS_ARCH),WINNT)
CSRCS += \
		msmap2tsv.c \
		msdump2symdb.c \
		$(NULL)
else
CSRCS += \
		nm2tsv.c \
		$(NULL)
endif

SIMPLE_PROGRAMS	= $(CSRCS:.c=$(BIN_SUFFIX))

include $(topsrcdir)/config/config.mk

ifeq ($(OS_ARCH),WINNT)
OS_LIBS += \
		imagehlp.lib \
		$(NULL)
endif


include $(topsrcdir)/config/rules.mk
