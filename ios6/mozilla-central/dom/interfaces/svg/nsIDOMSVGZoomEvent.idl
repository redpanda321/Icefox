interface nsIDOMSVGRect;

[scriptable, builtinclass, uuid(9cf032c6-8485-4bde-90d4-0c8bbff27a12)]
interface nsIDOMSVGZoomEvent : nsIDOMUIEvent
{
  readonly attribute nsIDOMSVGRect  zoomRectScreen;
