EXPORTS_NAMESPACES = mozilla/scache
EXPORTS_mozilla/scache = StartupCache.h \
  StartupCacheUtils.h \
  $(NULL)

XPIDLSRCS = nsIStartupCache.idl \
  $(NULL)

include $(topsrcdir)/config/rules.mk
