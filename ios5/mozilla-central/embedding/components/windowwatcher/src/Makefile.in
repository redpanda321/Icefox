DEPTH		= ../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= embedcomponents
LIBRARY_NAME	= windowwatcher_s
LIBXUL_LIBRARY	= 1

CPPSRCS		= \
                  nsWindowWatcher.cpp  \
                  nsAutoWindowStateHelper.cpp \
                  $(NULL)

